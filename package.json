{
  "name": "yt-scanner",
  "version": "1.0.0",
  "description": "Event-driven YouTube video scanner for detecting Amazon gift coupons",
  "main": "dist/bootstrap.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/bootstrap.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "check-all": "bash scripts/check-types.sh",
    "test:channel": "npm run build && node scripts/test-channel-monitoring.js",
    "test:d1": "npm run build && node scripts/test-cloudflare-d1.js",
    "docker:build": "docker build -t yt-scanner .",
    "docker:run": "docker run -p 3000:3000 yt-scanner",
    "setup": "npm install && cp env.example .env && mkdir -p logs temp credentials"
  },
  "keywords": [
    "youtube",
    "ocr",
    "video-analysis",
    "serverless",
    "event-driven"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "@google-cloud/firestore": "^7.11.1",
    "@google-cloud/pubsub": "^4.0.7",
    "@google-cloud/speech": "^6.0.0",
    "@google-cloud/tasks": "^6.1.0",
    "@google-cloud/video-intelligence": "^5.0.0",
    "@google-cloud/vision": "^4.3.3",
    "@types/module-alias": "^2.0.4",
    "@types/xml2js": "^0.4.14",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "googleapis": "^128.0.0",
    "helmet": "^7.1.0",
    "module-alias": "^2.2.3",
    "ngrok": "^5.0.0-beta.2",
    "node-cron": "^3.0.3",
    "sharp": "^0.33.1",
    "winston": "^3.11.0",
    "wrangler": "^3.84.1",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
